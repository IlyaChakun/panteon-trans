{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "908085288135.dkr.ecr.us-east-2.amazonaws.com/nginx:nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "config"
      ],
      "memory": 100
    },
    {
      "name": "config",
      "image": "908085288135.dkr.ecr.us-east-2.amazonaws.com/config:config",
      "hostname": "config",
      "essential": false,
      "memory": 256
    }
  ]
}